    "test:c": "yarn node --loader ts-node/esm --no-warnings=ExperimentalWarning ./problems/c/index.ts < ./problems/c/input.txt",
    "test:d": "yarn node --loader ts-node/esm --no-warnings=ExperimentalWarning ./problems/d/index.ts < ./problems/d/input.txt",
    "test:e": "yarn node --loader ts-node/esm --no-warnings=ExperimentalWarning ./problems/e/index.ts < ./problems/e/input.txt",
    "test:f": "yarn node --loader ts-node/esm --no-warnings=ExperimentalWarning ./problems/f/index.ts < ./problems/f/input.txt"
